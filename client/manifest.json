{
  "manifest_version": 2,

  "name": "Extension",
  "description": "Make browser talkable and upload some info to server.",
  "version": "1.0",

  "permissions": [
    "ws://127.0.0.1:5678/*",    
    "tabs",
    "notifications"
  ],
  "page_action": {
		"default_icon": {
			"19": "images/icon.png",
			"38": "images/icon.png"
		},
		"default_title": "information", // shown in tooltip
		"default_popup": "popup.html"
	},
  "background": { "scripts": ["js/jquery-2.1.1.min.js" ,"background.js"] },
  "content_scripts": [{
    "matches": ["https://*/*","http://*/*"],
    "js": ["content_script.js", "js/jquery-2.1.1.min.js"]
  }]  
}